			<true/>
			<key>label</key>
			<string>Modules</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<false/>
			<key>defaults</key>
			<string>com.peterdev.notchcontrol</string>
			<key>label</key>
			<string>Grabber</string>
